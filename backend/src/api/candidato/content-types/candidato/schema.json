{
  "kind": "collectionType",
  "collectionName": "candidatos",
  "info": {
    "singularName": "candidato",
    "pluralName": "candidatos",
    "displayName": "Candidato",
    "description": "Candidatos para votaciones de tokens"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "descripcion": {
      "type": "text",
      "required": true,
      "maxLength": 500
    },
    "imagen": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "votos": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "activo": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "fechaCreacion": {
      "type": "datetime",
      "required": true
    },
    "categoria": {
      "type": "enumeration",
      "enum": ["meme", "utility", "gaming", "defi"],
      "default": "meme"
    },
    "mintAddress": {
      "type": "string",
      "maxLength": 50
    }
  }
}