{
  "kind": "collectionType",
  "collectionName": "proyectos_next",
  "info": {
    "singularName": "proyecto-next",
    "pluralName": "proyectos-next",
    "displayName": "Proyecto Next",
    "description": "Pr√≥ximos proyectos de tokens a lanzar"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "descripcion": {
      "type": "text",
      "required": true,
      "maxLength": 500
    },
    "imagen": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "fechaLanzamiento": {
      "type": "datetime",
      "required": true
    },
    "estado": {
      "type": "enumeration",
      "enum": ["proximo", "en-desarrollo", "lanzado", "cancelado"],
      "default": "proximo",
      "required": true
    },
    "destacado": {
      "type": "boolean",
      "default": false
    },
    "progreso": {
      "type": "integer",
      "min": 0,
      "max": 100,
      "default": 0
    },
    "categoria": {
      "type": "enumeration",
      "enum": ["meme", "utility", "gaming", "defi"],
      "default": "meme"
    },
    "equipo": {
      "type": "string",
      "maxLength": 200
    },
    "sitioWeb": {
      "type": "string",
      "maxLength": 200
    },
    "twitter": {
      "type": "string",
      "maxLength": 200
    }
  }
}